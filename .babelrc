{
  "presets": [
    [
      "next/babel",
      {
        "styled-jsx": {
          "plugins": [
            "styled-jsx-plugin-postcss"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "transform-flow-strip-types",
    [
      "module-resolver", {
        "root": ["./"],
        "alias": {
          "components": "./src/components",
          "styles": "./src/styles",
          "store": "./src/store",
          "models": "./src/models",
          "i18n": "./src/i18n",
          "store": "./src/store"
        }
      }
    ]
  ]
}
